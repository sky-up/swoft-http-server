{
  "name": "swoft-fork/http-server",
  "type": "library",
  "version": "v2.1.0",
  "keywords": [
    "php",
    "swoole",
    "swoft"
  ],
  "description": "swoft http server component",
  "homepage": "https://github.com/sky-up/swoft-http-server",
  "license": "Apache-2.0",
  "require": {
    "swoft-fork/framework": "~2.1.0",
    "swoft-fork/http-message": "~2.1.0",
    "swoft-fork/validator": "~2.1.0"
  },
  "autoload": {
    "psr-4": {
      "Swoft\\Http\\Server\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SwoftTest\\Http\\Server\\Unit\\": "test/unit/",
      "SwoftTest\\Http\\Server\\Testing\\": "test/testing/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5 || ^8.0"
  },
  "scripts": {
    "test": "./vendor/bin/phpunit -c phpunit.xml"
  }
}
